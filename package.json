  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "nodemon --ext ts --watch ./src/server --exec pnpm -- run server",
    "generate-tsconfig-paths": "ts-node config/generateTsConfigPaths.ts",
    "knex": "ts-node node_modules/knex/bin/cli.js",
    "lint:eslint": "eslint .",
    "lint": "pnpm run lint:eslint && pnpm run type-check",
    "prepush": "pnpm run lint && pnpm run test",
    "prod": "pnpm run build && pnpm run server:prod",
    "server:prod": "NODE_ENV=production pnpm run server",
    "server": "ts-node ./src/server/server.ts",
    "test:content": "http-server tests/content -a localhost -p ${CONTENT_PORT:-8001} --cors",
    "test:integration": "VITEST_MODE=integration CONTENT_PORT=9876 INNODOC_CONTENT_ROOT=http://localhost:9876/ start-server-and-test test:content http://localhost:9876 test:vitest",
    "test:unit": "VITEST_MODE=unit pnpm run test:vitest",
    "test:vitest": "vitest run",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "type-check": "tsc -p ."
  },
  "imports": {
    "#build/*": "./build/*",
    "#constants": "./src/constants.ts",
    "#pages/*": "./src/pages/*",
    "#renderer/*": "./src/renderer/*",
    "#routes": "./src/routes.ts",
    "#server/*": "./src/server/*",
    "#store/*": "./src/store/*",
    "#test-utils": "./tests/integration/utils.tsx",
    "#types/*": "./src/types/*",
    "#ui/*": "./src/ui/*",
    "#utils/*": "./src/utils/*"
  },
  "pnpm": {
    "overrides": {
      "@emotion/react": "^11.10.5",
      "@reduxjs/toolkit": "^1.9.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-redux": "^8.0.5",
      "redux": "^4.2.0"
    }
  }
