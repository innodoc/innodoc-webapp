  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "pnpm run server",
    "lint:eslint": "eslint .",
    "lint": "pnpm lint:eslint && pnpm type-check",
    "prod": "pnpm run build && pnpm run server:prod",
    "server:prod": "NODE_ENV=production ts-node ./src/server/server.ts",
    "server": "ts-node ./src/server/server.ts",
    "test:e2e:content": "http-server e2e/content -a localhost -p ${CONTENT_PORT:-8001} --cors",
    "type-check": "tsc -p ."
  },
  "imports": {
    "@/*": "./src/*.ts"
  },
  "pnpm": {
    "overrides": {
      "@reduxjs/toolkit": "^1.8.3",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-redux": "^8.0.2",
