{
  "name": "innodoc-webapp",
  "version": "0.0.2-alpha",
  "author": "innoCampus, TU Berlin",
  "contributors": [
    "Mirko Dietrich <dietrich@math.tu-berlin.de>"
  ],
  "description": "innoDoc web app",
  "main": "index.js",
  "repository": "https://gitlab.tu-berlin.de/innodoc/innodoc-webapp",
  "files": [
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {
    "build": "next build src",
    "build:bundle-analyze": "BUNDLE_ANALYZE=both yarn build",
    "server": "node server/server.js",
    "dev": "NODE_ENV=development yarn run server",
    "start": "NODE_ENV=production yarn run server",
    "lint": "eslint src server e2e",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "yarn run test:unit --watch",
    "test:unit:coverage": "yarn run test:unit --coverage",
    "test:e2e": "PROD_PORT=${PROD_PORT:-6123} CONTENT_PORT=${CONTENT_PORT:-6124} CONTENT_ROOT=${CONTENT_ROOT:-http://localhost:6124/} jest --runInBand --forceExit --detectOpenHandles --config e2e/jest.e2e.config.js",
    "test:e2e:show": "PUPPETEER_HEADLESS=false yarn run test:e2e",
    "test:e2e:content": "http-server e2e/content -a localhost -p ${CONTENT_PORT:-8001} --cors"
  },
  "keywords": [
    "innodoc",
    "react",
    "elearning"
  ],
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@svgr/webpack": "^4.3.2",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.22.0",
    "antd-scss-theme-plugin": "^1.0.7",
    "classnames": "^2.2.6",
    "dotenv-safe": "^6.1.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "i18next": "^17.0.11",
    "i18next-browser-languagedetector": "^3.0.3",
    "i18next-express-middleware": "^1.7.3",
    "i18next-node-fs-backend": "^2.1.2",
    "i18next-xhr-backend": "^3.1.2",
    "ignore-loader": "^0.1.2",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "load-script": "^1.0.0",
    "md5": "^2.2.1",
    "nerdamer": "^0.8.4",
    "next": "^9.0.3",
    "next-i18next": "^1.1.0",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "node-sass": "^4.12.0",
    "object-hash": "^1.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.0.1",
    "redux": "^4.0.4",
    "redux-orm": "^0.13.2",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "sass-loader": "^7.2.0",
    "url-loader": "^2.1.0",
    "use-media": "^1.4.0",
    "webpack": "^4.39.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "ecstatic": "^4.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.0",
    "jest-enzyme": "^7.1.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "1.19.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga-test-plan": "4.0.0-beta.4"
  }
}
