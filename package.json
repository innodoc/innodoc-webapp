    "i18next-express-middleware": "^1.7.3",
    "i18next-node-fs-backend": "^2.1.2",
    "i18next-xhr-backend": "^2.0.1",
    "ignore-loader": "^0.1.2",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "load-script": "^1.0.0",
    "mathjs": "^5.8.0",
    "md5": "^2.2.1",
