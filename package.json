  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "dev": "nodemon --ext ts --watch ./src/server --exec pnpm -- run server",
    "dev:mock": "INNODOC_API_MOCK=true INNODOC_DEFAULT_COURSE_SLUG=testcourse INNODOC_COURSE_SLUG_MODE=DISABLED pnpm run dev",
    "generate-tsconfig-paths": "ts-node config/generateTsConfigPaths.ts",
    "knex": "ts-node node_modules/knex/bin/cli.js",
    "import-v1": "ts-node src/server/commands/importer/cli.ts",
    "lint:eslint": "eslint .",
    "lint": "pnpm run lint:eslint && pnpm run type-check",
    "prepush": "pnpm run lint && pnpm run test",
    "prod": "pnpm run build && pnpm run server:prod",
    "routes": "ts-node src/server/commands/routes/cli.ts",
    "server:prod": "NODE_ENV=production pnpm run server",
    "server": "ts-node ./src/server/server.ts",
    "test:e2e": "playwright test",
    "test:integration": "VITEST_MODE=integration pnpm run test:vitest",
    "test:unit": "VITEST_MODE=unit pnpm run test:vitest",
    "test:vitest": "vitest run",
    "test": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "type-check": "tsc -p .",
    "visualize-bundle": "VISUALIZE_BUNDLE=true pnpm run build"
  },
  "imports": {
    "#build/*": "./build/*",
    "#constants": "./src/constants.ts",
    "#markdown/*": "./src/markdown/*",
    "#pages/*": "./src/pages/*",
    "#renderer/*": "./src/renderer/*",
    "#routes/*": "./src/routes/*",
    "#server/*": "./src/server/*",
    "#store/*": "./src/store/*",
    "#test-utils": "./tests/integration/utils.tsx",
    "#types/*": "./src/types/*",
    "#ui/*": "./src/ui/*",
    "#utils/*": "./src/utils/*"
  },
  "pnpm": {
    "overrides": {
      "@emotion/react": "^11.10.6",
      "@reduxjs/toolkit": "^1.9.3",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-redux": "^8.0.5",
      "redux": "^4.2.1"
