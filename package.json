{
  "name": "innodoc",
  "version": "0.2.3",
  "scripts": {
    "build": "pnpm run build:clean && pnpm run build:icon-bundle && pnpm run build:client && pnpm run build:server",
    "build:clean": "rimraf dist",
    "build:client": "vite build --mode production",
    "build:icon-bundle": "ts-node scripts/buildIconBundle/buildIconBundle.ts",
    "build:server": "tsup",
    "dev": "nodemon --ext ts --watch ./src/server --exec pnpm -- run server:dev",
    "dev:mock": "INNODOC_API_MOCK=true INNODOC_DEFAULT_COURSE_SLUG=testcourse INNODOC_COURSE_SLUG_MODE=DISABLED nodemon --ext ts --watch ./src/server --exec ts-node ./src/server/startMockServer.ts",
    "generate-paths": "ts-node scripts/generateTsConfigPaths.ts",
    "knex": "ts-node node_modules/knex/bin/cli.js",
    "import-v1": "ts-node src/server/commands/importer/cli.ts",
    "lint:eslint": "eslint .",
    "lint": "pnpm run lint:eslint && pnpm run type-check",
    "prepush": "pnpm run lint && pnpm run test",
    "prod": "pnpm run build && pnpm run server:prod",
    "routes": "ts-node src/server/commands/routes/cli.ts",
    "server:prod": "NODE_ENV=production pnpm node dist/startServer.js",
    "server:dev": "ts-node ./src/server/startServer.ts",
    "test:e2e": "playwright test",
    "test:integration": "VITEST_MODE=integration pnpm run test:vitest",
    "test:unit": "VITEST_MODE=unit pnpm run test:vitest",
    "test:vitest": "vitest run",
    "test": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "type-check": "tsc -p .",
    "visualize-bundle": "VISUALIZE_BUNDLE=true pnpm run build"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/lato": "^5.0.5",
    "@iconify-json/mdi": "^1.1.53",
    "@iconify/utils": "2.1.7",
    "@mui/lab": "5.0.0-alpha.136",
    "@mui/material": "^5.14.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "camelcase": "^7.0.1",
    "camelcase-keys": "^8.0.2",
    "clsx": "^1.2.1",
    "crc": "^4.3.2",
    "decode-named-character-reference": "^1.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "hast-util-is-element": "^2.1.3",
    "hastscript": "^7.2.0",
    "i18next": "^23.2.11",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-backend": "^2.2.1",
    "iso-639-1": "^2.1.15",
    "katex": "^0.16.8",
    "knex": "^2.5.1",
    "locale": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "mdast-builder": "^1.1.1",
    "mdast-util-gfm-strikethrough": "^1.0.3",
    "mdast-util-gfm-table": "^1.0.7",
    "mdast-util-mdx-jsx": "^2.1.4",
    "mdast-util-to-hast": "^12.3.0",
    "micromark-extension-gfm-strikethrough": "^1.0.7",
    "micromark-extension-gfm-table": "^1.0.7",
    "micromark-extension-mdx-jsx": "^1.0.5",
    "micromark-extension-mdx-md": "^1.0.1",
    "pandoc-filter": "^2.1.0",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.11.1",
    "pg-protocol": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^13.0.2",
    "react-katex": "^3.0.1",
    "react-redux": "^8.1.1",
    "react-transition-group": "^4.4.5",
    "rehype-katex": "^6.0.3",
    "rehype-react": "^7.2.0",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.1.0",
    "remark-heading-id": "^1.0.0",
    "remark-inline-links": "^6.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "remark-squeeze-paragraphs": "^5.0.1",
    "remark-stringify": "^10.0.3",
    "sirv": "^2.0.3",
    "ts-node": "^10.9.1",
    "type-fest": "^3.13.0",
    "typescript": "~5.1.6",
    "unified": "^10.1.2",
    "unist-util-is": "^5.2.1",
    "unist-util-visit": "^4.1.2",
    "vite": "^4.4.3",
    "vite-plugin-ssr": "^0.4.133",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@iconify/types": "^2.0.0",
    "@playwright/test": "^1.36.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.44.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/hast": "^2.3.4",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/katex": "^0.16.1",
    "@types/locale": "^0.1.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/mdast": "3.0.11",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "@types/react-transition-group": "^4.4.6",
    "@types/svg-parser": "^2.0.3",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@types/unist": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/utils": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-redux": "^4.0.0",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "hast-util-sanitize": "^4.1.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "svg-parser": "^2.0.4",
    "tsup": "^7.1.0",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "private": true,
  "type": "module",
  "imports": {
    "#icon-bundle": "./dist/iconBundle.json",
    "#constants": "./src/constants.ts",
    "#markdown/*": "./src/markdown/*",
    "#pages/*": "./src/pages/*",
    "#renderer/*": "./src/renderer/*",
    "#routes/*": "./src/routes/*",
    "#server/*": "./src/server/*",
    "#store/*": "./src/store/*",
    "#test-utils": "./tests/integration/utils.tsx",
    "#types/*": "./src/types/*",
    "#ui/*": "./src/ui/*",
    "#utils/*": "./src/utils/*"
  },
  "pnpm": {
    "overrides": {
      "@emotion/react": "^11.11.1",
      "@reduxjs/toolkit": "^1.9.5",
      "typescript": "~5.0.4",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-redux": "^8.0.7",
      "redux": "^4.2.1"
    },
    "patchedDependencies": {
      "hast-util-from-html-isomorphic@1.0.0": "patches/hast-util-from-html-isomorphic@1.0.0.patch",
      "decode-named-character-reference@1.0.2": "patches/decode-named-character-reference@1.0.2.patch"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
