{
  "name": "innodoc-webapp",
  "version": "0.0.1",
  "author": "innoCampus, TU Berlin",
  "contributors": [
    "Mirko Dietrich <dietrich@math.tu-berlin.de>"
  ],
  "description": "innoDoc web app",
  "main": "index.js",
  "repository": "https://gitlab.tu-berlin.de/innodoc/innodoc-webapp",
  "files": [
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {
    "build": "next build src",
    "server": "babel-node server/server.js",
    "dev": "NODE_ENV=development npm run server",
    "start": "NODE_ENV=production npm run server",
    "lint": "eslint src server",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:e2e:show": "NIGHTMARE_SHOW=true npm run test:e2e",
    "test:e2e:ci": "e2e/ci-e2e.sh",
    "test:e2e:content": "http-server e2e/content -a localhost -p ${CONTENT_PORT:-8001} --cors"
  },
  "keywords": [
    "innodoc",
    "react",
    "elearning"
  ],
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@babel/node": "^7.0.0",
    "@zeit/next-bundle-analyzer": "^0.1.1",
    "@zeit/next-sass": "^0.2.0",
    "classnames": "^2.2.6",
    "dotenv-safe": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "express": "^4.16.3",
    "i18next": "^11.6.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-express-middleware": "^1.3.2",
    "i18next-node-fs-backend": "^2.0.0",
    "i18next-xhr-backend": "^1.5.1",
    "isomorphic-unfetch": "^2.1.1",
    "load-script": "^1.0.0",
    "mathjs": "^5.1.1",
    "md5": "^2.2.1",
    "next": "^6.1.1",
    "next-redux-saga": "^3.0.0",
    "next-redux-wrapper": "^2.0.0",
    "nightmare": "^3.0.1",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.4.2",
    "react-i18next": "^7.11.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "semantic-ui-react": "^0.82.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "ecstatic": "^3.2.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-environment-enzyme": "^6.0.3",
    "jest-environment-node": "^23.4.0",
    "jest-enzyme": "^6.0.3",
    "redux-devtools-extension": "^2.13.5"
  }
}
