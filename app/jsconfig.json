{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "es6",
    "paths": {
      "@components/*": ["./components/*"],
      "@hooks/*": ["./hooks/*"],
      "@lib/*": ["./lib/*"],
      "@style/*": ["./style/*"]
    }
  },
  "include": ["next.config/**/*.js", "src/**/*.jsx", "src/**/*.js"],
  "exclude": ["analyze", "src/.next", "src/.next-dev", ".swc"]
}
