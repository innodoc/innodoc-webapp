{
  "search.exclude": {
    "packages/client-web/bundle-analyzer": true,
    "packages/client-web/src/.next*": true,
    "packages/client-web/src/public/locales/*/*.missing.json": true,
    "packages/server/dist": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "eslint.nodePath": ".yarn/sdks",
  "eslint.packageManager": "yarn",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "stylelint.packageManager": "yarn",
  "stylelint.validate": ["sss"]
}
