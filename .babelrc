{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": ["@babel/plugin-proposal-export-default-from"]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": ["@babel/plugin-proposal-export-default-from"]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": { "modules": "commonjs" }
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-export-default-from"]
    }
  }
}
